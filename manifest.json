{
	"manifest_version": 2,
	"name": "TimeTable",
	"version": "1.0",
	"description": "Get TimeTable from The Game",
	"icons": {
	  "48": "icons/border-48.png"
	},
	"permissions": [
	  "activeTab"
	],
	"content_scripts": [
	  {
		"matches": ["*://planning.thegame-france.com/*"],
		"js": ["getTimeTable.js"],
		"run_at": "document_end"
	  }
	],
	"background": {
	  "scripts": ["getTimeTable.js"],
	  "persistent": false
	}
  }
